<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/usercontainer"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:background="@drawable/myaccount_pattern"
            android:gravity="center"
            android:orientation="vertical">


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:id="@+id/below"
            android:layout_marginTop="70dp"
            android:orientation="horizontal">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageView58"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/person4" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/below"

            android:orientation="vertical">

            <TextView
                android:id="@+id/textView65"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="#201A16"
                android:layout_margin="10dp"
                android:textSize="18sp"

                android:text="Mark obama" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_margin="15dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView59"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="5dp"
                    app:srcCompat="@drawable/user" />

                <TextView
                    android:id="@+id/txtProfile"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="15dp"
                    android:textColor="#201A16"
                    android:text="My profile" />
            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#41000000"/>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_margin="15dp"
                android:orientation="horizontal">

                <ImageView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/empire1" />

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="15dp"
                    android:textColor="#201A16"
                    android:text="Register a property" />
            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#41000000"/>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_margin="15dp"
                android:orientation="horizontal">

                <ImageView

                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    app:srcCompat="@drawable/pay" />

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="15dp"
                    android:textColor="#201A16"
                    android:text="My cards" />
            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#41000000"/>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_margin="15dp"
                android:orientation="horizontal">

                <ImageView

                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    app:srcCompat="@drawable/tie1" />

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="15dp"
                    android:textColor="#201A16"
                    android:text="My Agent" />
            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#41000000"/>





            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_margin="15dp"
                android:orientation="horizontal">

                <ImageView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/groupp" />

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="15dp"
                    android:textColor="#201A16"
                    android:text="Evaluation list" />
            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#41000000"/>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_margin="15dp"
                android:orientation="horizontal">

                <ImageView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/calculator" />

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="15dp"
                    android:textColor="#201A16"
                    android:id="@+id/txtmortgagecalculator"
                    android:text="Mortgage calculator" />
            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#41000000"/>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_margin="15dp"
                android:orientation="horizontal">

                <ImageView

                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    app:srcCompat="@drawable/share1" />

                <TextView

                    android:layout_width="0dp"
                    android:id="@+id/textShare"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="15dp"
                    android:textColor="#201A16"
                    android:text="Introduce app to friends" />
            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#41000000"/>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:orientation="horizontal">

                <ImageView

                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:id="@+id/iconsettinguser"
                    app:srcCompat="@drawable/settings1" />

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:id="@+id/txtsettings"
                    android:layout_marginLeft="15dp"
                    android:textColor="#201A16"
                    android:text="Settings" />
            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#41000000"/>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:orientation="horizontal">

                <ImageView

                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    app:srcCompat="@drawable/support" />

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="15dp"
                    android:textColor="#201A16"
                    android:text="Customer Support" />
            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginBottom="-10dp"
                android:background="#41000000"/>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:orientation="horizontal">

                <ImageView

                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    app:srcCompat="@drawable/logout" />

                <TextView

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:id="@+id/txtLogoutMortgage"
                    android:layout_marginLeft="15dp"
                    android:textColor="#201A16"
                    android:text="Logout" />
            </LinearLayout>







        </LinearLayout>

    </RelativeLayout>

</FrameLayout>